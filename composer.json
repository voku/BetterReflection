{
    "name": "voku/better-reflection",
    "description": "Better Reflection - an improved code reflection API",
    "license": "MIT",
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "jetbrains/phpstorm-stubs": "dev-master#312fcf779d5bcec5b4059b9e45ba11e575a9aacf",
        "nikic/php-parser": "~4.10",
        "phpdocumentor/reflection-docblock": "~5.2",
        "phpdocumentor/type-resolver": "~1.4",
        "roave/signature": "~1.3"
    },
    "authors": [
        {
            "name":     "James Titcumb",
            "email":    "james@asgrim.com",
            "homepage": "https://github.com/asgrim"
        },
        {
            "name":     "Marco Pivetta",
            "email":    "ocramius@gmail.com",
            "homepage": "https://ocramius.github.io/"
        },
        {
            "name":     "Gary Hockin",
            "email":    "gary@roave.com",
            "homepage": "https://github.com/geeh"
        },
        {
            "name":     "Jaroslav Hansl√≠k",
            "email":    "kukulich@kukulich.cz",
            "homepage": "https://github.com/kukulich"
        }

    ],
    "require-dev": {
        "phpstan/phpstan": "~0.12",
        "phpunit/phpunit": "~7.5",
        "vimeo/psalm": "~3.18"
    },
    "autoload": {
        "psr-4": {
            "Roave\\BetterReflection\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Roave\\BetterReflectionTest\\": "test/unit",
            "Roave\\BetterReflectionBenchmark\\": "test/benchmark"
        }
    },
    "suggest": {
        "composer/composer": "Required to use the ComposerSourceLocator"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
